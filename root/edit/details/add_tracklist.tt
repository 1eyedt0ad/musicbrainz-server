<table class="tbl">
  <thead>
    <tr>
      <th>[% l('#') %]</th>
      <th>[% l('Name') %]</th>
      <th>[% l('Artist Credit') %]</th>
      <th>[% l('Length') %]</th>
    </tr>
  </thead>
  [% FOR track=edit.display_data.tracks %]
  <tr>
    <td>[% track.position %]</td>
    <td>[% track.name | html %]</td>
    <td>[% artist_credit(track.artist_credit) %]</td>
    <td>[% track.length | format_length %]</td>
  </tr>
  [% END %]
</table>
