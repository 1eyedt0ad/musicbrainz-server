<table class="details">
  <tr>
    <th>[% l('Release:') %]</th>
    <td colspan="2">[% l('{release} by {artist}',
             { release => link_release(edit.display_data.release),
               artist  => artist_credit(edit.display_data.release.artist_credit) }) %]</td>
  </tr>
  <tr>
    <th>[% l('Quality:') %]</th>
    <td class="old">[% quality_name(edit.display_data.quality.old) %]</td>
    <td class="new">[% quality_name(edit.display_data.quality.new) %]</td>
  </tr>
</table>
