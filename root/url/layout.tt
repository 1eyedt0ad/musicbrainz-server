[% WRAPPER 'layout.tt' %]

    [% WRAPPER 'layout/sidebar.tt' %]
        <ul class="links">
            [% IF c.user_exists %]
                <li>[% use_in_relationship(url) %]</li>
                <li>[% relate_to_ellipsis(url) %]</li>
                <li>[% link_entity(url, 'edits', l('Editing history')) %]</li>
            [% ELSE %]
                <li>[% request_login(l('Log in to edit')) %]</li>
            [% END %]
        </ul>
    [% END %]

    <div id="content">
        [% INCLUDE 'url/header.tt' %]
        [% content %]
    </div>
[% END %]
