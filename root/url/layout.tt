[% WRAPPER 'layout.tt' canonical_url=replace_gid(entity.gid) %]
    [%- IF !full_width -%]
        [% WRAPPER 'layout/sidebar/shared-entity-sidebar.tt' entity=url %][%~ END ~%]
    [%- END -%]

    <div id="content">
         [% INCLUDE 'url/header.tt' %]
         [% content %]
    </div>
[% END %]
